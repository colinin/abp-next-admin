{
  "ReverseProxy": {
    "Routes": {
      "abp-route": {
        "ClusterId": "abpCluster",
        "Match": {
          "Path": "/api/abp/{**everything}"
        }
      },
      "token-route": {
        "ClusterId": "tokenCluster",
        "Match": {
          "Path": "/connect/{**everything}"
        },
        "Transforms": [
          {
            "RequestHeaderOriginalHost": "true"
          }
        ]
      },
      "account-route": {
        "ClusterId": "accountCluster",
        "Match": {
          "Path": "/api/account/{**everything}"
        }
      },
      "identity-route": {
        "ClusterId": "identityCluster",
