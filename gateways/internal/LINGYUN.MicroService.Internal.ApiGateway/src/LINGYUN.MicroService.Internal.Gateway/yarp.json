        }
      },
      "im-route": {
        "ClusterId": "im-cluster",
        "Match": {
          "Path": "/api/im/{**everything}"
        }
      },
      "notifications-route": {
        "ClusterId": "notifications-cluster",
        "Match": {
          "Path": "/api/notifications/{**everything}"
        }
      }
    },
    "Clusters": {
